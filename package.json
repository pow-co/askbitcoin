{
  "name": "askbitcoin",
  "version": "0.2.0",
  "private": false,
  "description": "Ask Bitcoin a Question - Receive Answers with Proof of Work",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "start": "ts-node src/main.ts",
    "test": "NODE_ENV=test mocha --require=ts-node/register --timeout=200001 test/*_test.ts",
    "runtest": "NODE_ENV=test mocha --require=ts-node/register --timeout=20000 $1",
    "build": "tsc",
    "postbuild": "shebang-trim dist/bin/cli.js && chmod +x dist/bin/cli.js",
    "ci": "circleci local execute --job build",
    "commit": "npx cz",
    "build_web_ui": "cd web-ui && npm run build",
    "db:migrate": "sequelize db:migrate",
    "mine": "ts-node miner/main.ts",
    "docker:build": "docker build -t proofofwork/askbitcoin .",
    "docker:push": "docker push proofofwork/askbitcoin"
  },
  "bin": {
    "askbitcoin": "./dist/bin/cli.js"
  },
  "keywords": [],
  "author": "Zyler",
  "license": "",
